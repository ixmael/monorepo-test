{
  "name": "@bayonet/web",
  "version": "3.0.1-alpha.1",
  "main": "dist/analytics.js",
  "module": "dist/analytics.ems.js",
  "types": "dist/types.d.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "contributors": [
    "Imran Arshad <imran@bayonet.io>",
    "Ismael Ramos <ismael.ramos@bayonet.io>"
  ],
  "scripts": {
    "build": "npm run clean Â­&& rollup -c config/rollup/production.js --environment INCLUDE_DEPS,BUILD:production && npm run build:types",
    "build:types": "npm run clean:types && dts-bundle-generator src/index.ts -o dist/types.d.ts",
    "server": "npm run test:clean && NODE_ENV=development rollup -w -c config/rollup/development.js --environment INCLUDE_DEPS,BUILD:development",
    "tests": "npm run test:unit && npm run test:integration && npm run test:integration:firefox",
    "test:build": "npm run test:clean && NODE_ENV=test rollup -w -c config/rollup/development.js --environment INCLUDE_DEPS,BUILD:development",
    "test:unit": "NODE_ENV=test ts-mocha -p ./tsconfig.test.json -r tsconfig-paths/register -r jsdom-global/register src/**/__tests__/**/*.spec.ts",
    "test:integration": "NODE_ENV=test start-server-and-test test:start-server-api http://localhost:3200 test:build-cypress",
    "test:integration:firefox": "NODE_ENV=test start-server-and-test test:start-server-api http://localhost:3200 test:build-cypress:firefox",
    "test:build-cypress": "NODE_ENV=test start-server-and-test test:build http://localhost:3100 'cypress run --spec tests/e2e/integration/**/*.spec.js'",
    "test:build-cypress:firefox": "NODE_ENV=test start-server-and-test test:build http://localhost:3100 'cypress run --browser firefox --spec tests/e2e/integration/**/*.spec.js'",
    "test:start-server-api": "mocks-server --no-cli",
    "test:cypress": "NODE_ENV=test cypress open",
    "test:clean": "rimraf public/js/analytics.js",
    "clean": "rimraf dist",
    "clean:types": "rimraf dist/types.d.ts"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro": "3.6.1",
    "rollup-plugin-execute": "^1.1.1"
  },
  "devDependencies": {
    "@mocks-server/main": "^2.4.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/chai": "^4.2.22",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "chai": "^4.3.4",
    "cypress": "^9.5.1",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "esm": "^3.2.25",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.4.0",
    "mocha-sinon": "^2.1.2",
    "moq.ts": "^7.3.4",
    "rollup": "^2.57.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-pluginutils": "^2.8.2",
    "sinon": "^11.1.2",
    "start-server-and-test": "^1.14.0",
    "test-console": "^2.0.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "repository": {
    "type": "git",
